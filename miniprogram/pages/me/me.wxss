.unlogin-body {
  background-color: white;
  height: 100vh;
  width: 100vw;
}

.unlogin {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-top: 40%;
}
.unlogin .unlogin-image {
  width: 100rpx;
  height: 100rpx;
}
.unlogin .unlogin-button{
  width: 85%;
  margin-top: 50rpx;
  width: 80%;
}

.info {
  background-color: grey;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 100rpx 0;
}
.info .avatarUrl {
  width: 200rpx;
  height: 200rpx;
}
.info .nickName {
  color: black;
  margin: 30rpx;
}

.cell-body {
  position: relative;
}
.cell-list{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.cell {
  display: flex;
  justify-content: space-between;
  width: 85%;
  background-color: white;
  padding: 30rpx 20rpx ;
  border-radius: 20rpx;
  align-items: center;
  margin-bottom: 20rpx;
}
.cell-left .cell-left-text {
  margin-left: 20rpx;

}
.cell-left {
  display: flex;
  align-items: center;
}

.cell-left .cell-left-image {
  width: 50rpx;
  height: 50rpx;
}

.cell-right .cell-right-image {
  width: 40rpx;
  height: 40rpx;
}