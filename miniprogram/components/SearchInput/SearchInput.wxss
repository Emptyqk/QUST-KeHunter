.search {
  background-color:white;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.search-input {
  height: 80rpx;
  background-color: #f3f3f3;
  width: 90%;
  border-radius: 40rpx;
  color: black;
  display: flex;
  align-items: center;
  justify-content: center;
}
.search-content {
  display: flex;
  align-items: center;
  justify-content: center;
}
.search-content .search-icon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 30rpx;
  height: 30rpx;
}
.search-content .search-text {
  margin-left: 10rpx;
}